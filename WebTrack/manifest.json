{
    "name": "WebTrack",
    "description": "Keep track of your website usage times!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/icons/icon_v2_no_bg.png"
    }
}